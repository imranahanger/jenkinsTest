pipeline {

    agent any
    
    stages{
        
        stage("build"){

            steps {
                echo "this is build stage"
            }
        }
    }

    stages{
        
        stage("test"){

            steps {
                echo "this is test stage"
            }
        }
    }

    stages{
        
        stage("deploy"){

            steps {
                echo "this is test deployment stage"
            }
        }
    }
}